<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.big15.tradingweb.mapper.webData.FsDataListMapper">
	<select id="stockList" resultType="com.big15.tradingweb.dto.FsListDto">
		select *
		from fslist
		where stockType = '보통주'
		order by stockCode asc
	</select>
</mapper>

